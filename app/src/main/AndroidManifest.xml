<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appbar">

    <application
        android:allowBackup="true"
        android:icon="@drawable/user"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Menu"/>
        <activity android:name=".FirstActivity"/>
        <activity android:name=".SplashScreenActivity"/>
        <activity android:name=".UniversalActivity"/>
        <activity android:name=".TransitionActivity"/>
        <activity android:name=".MonitoringActivity"/>
        <activity android:name=".IndicatorActivity"/>
        <activity android:name=".PressureActivity"/>
        <activity android:name=".CheckboxActivity"/>
        <activity android:name=".SpinnerActivity"/>
        <activity android:name=".CalendarActivity"/>
        <activity android:name=".NotesActivity"/>
    </application>

</manifest>